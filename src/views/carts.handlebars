<header>
  <nav>
    <h1>Desafio</h1>
    <ul>
      <li>
        <a href="/">Home</a>
      </li>
      <li>
        <a href="/realTimeProducts">Real Time Products</a>
      </li>
    </ul>
  </nav>
  <hr />
</header>
<main>
  <article id="products">

    {{#each cart}}
      <div class="card cardEcommerce" style="width: 370px;height: 600px">
        <img
          src="
      "
          class="card-img-top style="
          height="300rem"
          alt={{product.description}}
        />
        <div class="card-body">
          <p class="card-title fw-bold fs-5">{{product.title}}</p>
          <p class="card-text"><small class="text-body-secondary">
              {{product.description}}
            </small></p>
          <p class="card-text precio fw-bold">${{product.price}}</p>
          <p class="card-text"><small class="text-body-secondary">
              quantity:
              {{quantity}}
            </small></p>

        </div>
      </div>

    {{/each}}

  </article>
</main>