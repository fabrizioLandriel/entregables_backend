<header>
  <nav>
    <h1>Desafio</h1>
    <ul>
      <li>
        <a href="/">Home</a>
      </li>
      <li>
        <a href="/realTimeProducts">Real Time Products</a>
      </li>
    </ul>
  </nav>
  <hr />
</header>
<main class="overflow">

  <h2>Usuario {{usuario.nombre}}</h2>
<hr>
<h3>Datos usuario:</h3>
<p>Nombre: <strong>{{usuario.nombre}}</strong></p>
<p>Correo electr√≥nico: <strong>{{usuario.email}}</strong></p>
<br><br>

  <article id="products">

    {{#each products}}
      <div class="card cardEcommerce" style="width: 370px;height: 600px">
        <img
          src="
      "
          class="card-img-top style="
          height="300rem"
          alt={{description}}
        />
        <div class="card-body">
          <p class="card-title fw-bold fs-5">{{title}}</p>
          <p class="card-text"><small class="text-body-secondary">
              {{description}}
            </small></p>
          <p class="card-text precio fw-bold">${{price}}</p>
          <p class="card-text"><small class="text-body-secondary">
              stock:
              {{stock}}
            </small></p>
          <button id=`btnAdd-{{id}}` class="btn btn-success">Add</button>
        </div>
      </div>

    {{/each}}

    <div class="pagination">
    {{#if hasPrevPage}}
        <a href={{prevLink}}><button>Ant</button></a>
        {{else}}
        <button disabled>Ant</button>
    {{/if}}
        <p>Pagina:<strong>{{page}}</strong></p>
    {{#if hasNextPage}}
        <a href={{nextLink}}><button>Sig</button></a>
        {{else}}
        <button disabled class="disabled">Sig</button>
    {{/if}}
</div>

    

  </article>

  
</main>
